{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "0.1.0",
	"command": "gulp",
	"isShellCommand": true,
	"args": ["build"],
	"tasks": [
		{
			"taskName": "default",
			"args": [],
			"isBuildCommand": true,
			"isBackground": false,
			"problemMatcher": [
				"$tsc",
				"$tslint5",
				{
					"pattern":[
						{
							"regexp": "^ERROR: ?([^\\s].*)\\[(\\d+), ?(\\d+)\\]:\\s*(.*)$",
							"file": 1,
							"line": 2,
							"column": 3,
							"message": 4
						}
					]
				}
			]
		}
	]
}